#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/jsheely/Projects/Decentology/hyperverse-base/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/jsheely/Projects/Decentology/hyperverse-base/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/wait-on@6.0.1/node_modules/wait-on/bin/wait-on" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/wait-on@6.0.1/node_modules/wait-on/bin/wait-on" "$@"
fi
